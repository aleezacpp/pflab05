/*Write a program that checks if a number is divisible by both 3 and 5 using logical operators.*/
#include <stdio.h>
int main(){
    int num;
    printf("enter num : \n");
    scanf("%d", &num);
    if (num%3==0 && num%5==0) {
        printf("%d is divisible by 5 and 3. \n", num);
    }
    else printf("%d is not divisible by 5 and 3. \n", num);
    return 0;
}
