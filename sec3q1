/*Write a program using a ternary operator to find the maximum of two numbers.*/
#include <stdio.h>
int main(){
    int num1;
    printf("enter num1 : \n");
    scanf("%d", &num1);
    int  num2;
    printf("enter num2 : \n");
    scanf("%d", &num2);
    (num1>num2) ? printf("num1 is greater than num2. \n") : printf("num2 is greater than num1. \n");
    return 0;
}
